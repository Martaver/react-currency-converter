{
  "name": "react-currency-converter",
  "version": "0.7.0",
  "description": "Currency Converter - Example React Component in TypeScript",
  "main": "lib/app.jsx",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (http://piotrwitek.github.io/)",
  "homepage": "https://github.com/piotrwitek/react-currency-converter",
  "repository": "https://github.com/piotrwitek/react-currency-converter.git",
  "bugs": "https://github.com/piotrwitek/react-currency-converter/issues",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules dev .fusebox",
    "reinstall": "rm -rf node_modules && yarn install",
    "dev": "ts-node -P scripts/ scripts/dev.ts",
    "dist": "ts-node -P scripts scripts/dist.ts",
    "lib": "rm -rf lib && tsc --project src/ -t 'es5' -m 'commonjs'",
    "lint": "tslint --project src/",
    "tsc": "tsc --project src/ --noEmit",
    "tsc:watch": "tsc --project src/ --noEmit -w",
    "test": "jest --config jest.json",
    "test:update": "jest --config jest.json -u",
    "precommit": "yarn run lint",
    "prepush": "yarn run tsc & yarn run test",
    "deploy:init": "git clone https://github.com/piotrwitek/react-currency-converter --branch gh-pages dist",
    "deploy": "git checkout gh-pages && cd dist && git add --all && git commit -m \"New Release\" && git push"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-redux-typescript": "^2.2.0",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "reselect": "^2.5.4",
    "rxjs": "^5.2.0",
    "tslib": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.1",
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.16",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.37",
    "@types/shelljs": "^0.7.0",
    "fuse-box": "^1.3.127",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "shelljs": "^0.7.7",
    "ts-jest": "^19.0.0",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  }
}
